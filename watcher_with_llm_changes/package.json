{
  "name": "watcher",
  "displayName": "Watcher",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "watcher.login",
        "title": "Login to Extension"
      },
      {
        "command": "watcher.myPasteCommand",
        "title": "Custom Paste"
      },
      {
        "command": "watcher.myCopyCommand",
        "title": "Custom Copy"
      }
    ],
    "keybindings": [
      {
        "command": "watcher.myPasteCommand",
        "key": "ctrl+v",
        "mac": "cmd+v",
        "when": "editorTextFocus"
      },
      {
        "command": "watcher.myCopyCommand",
        "key": "ctrl+c",
        "mac": "cmd+c"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.99.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1"
  },
  "dependencies": {
    "axios": "1.9.0"
  }
}
